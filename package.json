{
  "name": "vite-plugin-html-sort-tags",
  "version": "0.1.1",
  "description": "Vite plugin for sorting head tags in HTML",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --fix './**/*.{js,ts}'",
    "prepack": "tsdown",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/vite-plugin-html-sort-tags.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "html"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CyanSalt/vite-plugin-html-sort-tags/issues"
  },
  "homepage": "https://github.com/CyanSalt/vite-plugin-html-sort-tags#readme",
  "peerDependencies": {
    "vite": ">=2.0.0"
  },
  "devDependencies": {
    "@cyansalt/eslint-config": "^4.0.0",
    "@cyansalt/tsconfig": "^3.0.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/lodash-es": "^4.17.12",
    "eslint": "^9.3.0",
    "lodash-es": "^4.17.21",
    "release-it": "^17.0.0",
    "tsdown": "^0.16.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.6"
  },
  "dependencies": {
    "node-html-parser": "^6.1.10"
  }
}
